"manifest_version": 2,
"name": "TAS-CAL",
"short_name": "Ext name",
"version": "0.1",
"default_locale": "ja_JP",
"description": "I want Tasukaru.",
"icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
},

"content_scripts": [
  {
    "matches": ["http://www.google.com/*"],
    "css": ["mystyles.css"],
    "js": ["jquery-3.4.1.min.js", "myscript.js"]
  }
]

"browser_action": {
    "default_icon": {
        "19": "icon19.png"
    },
    "default_popup": "popup.html"
}
