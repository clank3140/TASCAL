{
    "manifest_version": 2,
    "name": "TAS-CAL",
    "short_name": "Ext name",
    "version": "1.5",
    "description": "I want Tasukaru.",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },

    "content_scripts": [
        {
            "matches": ["https://www.youtube.com/watch*"],
            "css": ["style_youtube.css"],
            "js": ["jquery-3.4.1.min.js", "content_script.js"]
        },
        {
            "matches": ["https://www.pixiv.net/member_illust.*"],
            "css": ["style_pixiv.css"]
        }
    ],

    "browser_action": {
        "default_popup": "popup.html"
    }
}
